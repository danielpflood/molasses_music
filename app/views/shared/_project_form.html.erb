<%= form_for @project do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class="field">
   name: <%= f.text_area :name %>
  </div>
<div class="field">
   type: <%= f.text_area :project_type %>
  </div>
  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>
